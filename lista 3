using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Exe3Lista3
{
    internal class Program
    {
        static void Main(string[] args)
        {
            string f = "f";
            string m = "m";
            string e;


            do
            {
                Console.Clear();
                Console.WriteLine("Digite o seu sexo, sendo entre F ou M");
                e = Console.ReadLine();

            } while (e != f && e != m);
            Console.WriteLine("Condição correta!");
        }
    }
}

4-
using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Exe4Lista3
{
    internal class Program
    {
        static void Main(string[] args)
        {
            for (int e = 5; e <= 5; e++)
            {
                for (int d = 1; d <= 10; d++)
                {
                    Console.WriteLine(e + " x " + d + " = " + e * d);
                }
            }
            Console.Read();
        }
    
    }
}
